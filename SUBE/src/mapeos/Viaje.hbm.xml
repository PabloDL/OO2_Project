<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Viaje" table="viaje">
		<id column="idviaje" name="idViaje">
			<generator class="identity" />
		</id>
		<property column="tarifa" name="tarifa" type="float" />
		<property column="fechaYHora" name="fechaHoraInicio" type="calendar" />
		<many-to-one name = "sube" class = "datos.Sube" column = "idSube" not-null = "true"/>
		<one-to-one name = "transporte" class = "datos.Transporte" ></one-to-one>
	</class>
</hibernate-mapping>