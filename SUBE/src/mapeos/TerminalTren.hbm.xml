<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.TerminalTren" table="terminalTren">
		<id column="idTerminalTren" name="idTerminalTren">
			<generator class="identity" />
		</id>
		<property column="linea" name="linea" type="string" />
		<property column="ramal" name="ramal" type="string" />
		<property column="estacion" name="estacion" type="string" />
	</class>
</hibernate-mapping>