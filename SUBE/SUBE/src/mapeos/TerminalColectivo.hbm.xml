<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.TerminalColectivo" table="terminalColectivo">
		<id column="idTerminalColectivo" name="idTerminalColectivo">
			<generator class="identity" />
		</id>
		<property column="linea" name="linea" type="string" />
		<property column="ramal" name="ramal" type="string" />
		<property column="numero" name="numero" type="int" />
	</class>
</hibernate-mapping>